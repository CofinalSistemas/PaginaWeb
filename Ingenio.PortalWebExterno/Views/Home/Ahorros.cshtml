<!-- scrip para parallax-->
<script type="text/javascript">
    function parallax() {
        setpos("#pb0");
        setpos("#pb1");

    }



    function setpos(element, factor) {
        factor = factor || 2;

        var offset = $(element).offset();
        var w = $(window);

        var posx = (offset.left - w.scrollLeft()) / factor;
        var posy = (offset.top - w.scrollTop()) / factor;

        $(element).css('background-position', '100% ' + posy + 'px');

        // use this to have parralax scrolling vertical and horizontal
        //$(element).css('background-position', posx+'px '+posy+'px');
    }

    $(document).ready(function () {
        parallax();
    }).scroll(function () {
        parallax();
    });
</script>
<!--funcion cerrar del video-->
<script>
    var myVideo = document.getElementById("miv");
    $('#myModal').on('hide.bs.modal', function (e) {
        // do something...
        myVideo.pause();
    })

    function stop() {
        var myVideo = document.getElementById("miv");
        if (myVideo.play) {
            myVideo.pause();

        }
        else {
            myVideo.play();
        }
    }
</script>
