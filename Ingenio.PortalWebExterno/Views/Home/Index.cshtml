        <script>
            window.onload = function () {
                // Get the image and insert it inside the modal - use its "alt" text as a caption
                var img = document.getElementById('myImg');
                var modalImg = document.getElementById("img01");
                var captionText = document.getElementById("caption"); //mensaje
                // Get the modal
                var modal = document.getElementById('myModal');

                var wasClosed = sessionStorage.getItem('closed');
                if (@conf.ShowAgainAfterClosed.ToString().ToLower()==true)
                {
                    wasClosed = false;
                }
                if (wasClosed == null || !wasClosed) {
                    modal.style.display = "block"; //aca es none o lbock segun si se debe mostrar
                }

                modalImg.src = "/imagenes/modal.jpg"; //url
                captionText.innerHTML = "@conf.Message";
                // Get the <span> element that closes the modal
                var span = document.getElementsByClassName("close")[0];
                // When the user clicks on <span> (x), close the modal
                span.onclick = function () {
                    setClosedState(modal);
                }

                modal.onclick = function () {
                    setClosedState(modal);
                }


            }

            //set closes statr
            function setClosedState(modal) {
                modal.style.display = "none";
                 //creo la variable en session para que no la vuelva a mostrar
                        sessionStorage.setItem('closed', JSON.stringify(@conf.ShowAgainAfterClosed.ToString().ToLower()));

            }
        </script>
