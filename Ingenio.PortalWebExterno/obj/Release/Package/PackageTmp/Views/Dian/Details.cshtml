@model IEnumerable<Ingenio.BO.Ingenio.Param_dian>

@{
    Layout = "~/Views/Shared/layoutAdmin.cshtml";
    ViewBag.Title = "DIAN";
    ViewBag.titulo = "Parametrizacion de intereses";
    ViewBag.url = Url.Action("index", "home");
    ViewBag.activo = "Estados";
}
<section id="contenido">
    <div class="container">
        <!DOCTYPE html>

        <html>
        <head>
            <meta name="viewport" content="width=device-width" />
            <title>Details</title>
        </head>
        <body>
            <p>
                @*@Html.ActionLink("Create New", "Create")*@
            </p>
            <table class="table">
                <tr>
                    <th>
                        INTERES PAGADOS
                    </th>
                    <th>
                       INTERES DEDUCIBLES
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.INTERES_PAGADOS) %
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.INTERES_DEDUCIBLES) %
                        </td>
                        <td>
                            @Html.ActionLink("Actualizar", "Edit", new { id = item.ID }) |
                            @*@Html.ActionLink("Details", "Details", new { id = item.ID }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.ID })*@
                        </td>
                    </tr>
                }

            </table>
        </body>
    </html>
        </div></section>